file = {:home/arthur/Dropbox/ArthurLara/relatorios/PFC/Simula{\c{c}}{\~{a}}o/1402.7050.pdf:pdf},
pages = {1--15},
title = {{Tools for dynamics simulation of robots: a survey based on user feedback}},
url = {http://arxiv.org/abs/1402.7050},
year = {2014}
}
