@phdthesis{Castro2014,
abstract = {O controle cooperativo de Ve{\'{\i}}culos A{\'{e}}reos N{\~{a}}o Tripulados (VANTs) do tipo multic{\'{o}}ptero vem sendo recentemente investigado de forma intensa por diversos grupos de pesquisa mundo afora. Com o intuito de tornar eficiente o projeto e a avalia{\c{c}}{\~{a}}o de sistemas de controle cooperativo, prop{\~{o}}e-se neste trabalho um ambiente de simula{\c{c}}{\~{a}}o Software-In-the-Loop (SIL) para avaliar leis de controle de forma{\c{c}}{\~{a}}o em posi{\c{c}}{\~{a}}o de um time composto por tr{\^{e}}s octoc{\'{o}}pteros. O ambiente {\'{e}} constitu{\'{\i}}do de tr{\^{e}}s computadores conectados por uma rede de comunica{\c{c}}{\~{a}}o Ethernet. Cada computador executa dois softwares: o MATLAB/Simulink e o X-Plane. As leis de controle s{\~{a}}o implementadas em Simulink, enquanto a din{\^{a}}mica e o ambiente de voo s{\~{a}}o simulados em X-Plane. Para realizar a coopera{\c{c}}{\~{a}}o entre os controladores, os computadores trocam dados de posi{\c{c}}{\~{a}}o por meio dos diagramas em Simulink. Para testar o ambiente proposto, adota-se uma lei de controle de forma{\c{c}}{\~{a}}o em posi{\c{c}}{\~{a}}o utilizando a abordagem l{\'{\i}}der-seguidor. O ve{\'{\i}}culo l{\'{\i}}der se desloca em linha reta, enquanto os ve{\'{\i}}culos seguidores acompanham a posi{\c{c}}{\~{a}}o do l{\'{\i}}der, mantendo uma forma{\c{c}}{\~{a}}o triangular. O ambiente {\'{e}} validado atrav{\'{e}}s de compara{\c{c}}{\~{o}}es entre o modelo em X-Plane e o modelo anal{\'{\i}}tico. Os testes revelam que por meio do ambiente proposto {\'{e}} poss{\'{\i}}vel avaliar uma lei de controle de forma{\c{c}}{\~{a}}o em posi{\c{c}}{\~{a}}o.},
author = {de Castro, Davi Ferreira},
file = {:home/arthur/Dropbox/ArthurLara/relatorios/PFC/Simula{\c{c}}{\~{a}}o/X-plane/TeseDaviCastro.pdf:pdf},
school = {Instituto Tecnol{\'{o}}gico de Aeron{\'{a}}utica, S{\~{a}}p Jose do Campos},
title = {{Ambiente de simula{\c{c}}{\~{a}}o SIL para sistemas de controle de forma{\c{c}}{\~{a}}o em posi{\c{c}}{\~{a}}o para multic{\'{o}}pteros}},
year = {2014}
}
