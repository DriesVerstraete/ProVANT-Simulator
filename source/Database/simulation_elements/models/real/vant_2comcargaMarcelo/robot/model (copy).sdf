      <TopicSubscriber> TauR </TopicSubscriber> 
      <TopicPublisher> SensoraR </TopicPublisher> 
    </plugin> 

    <plugin filename="libgazebo_ros_servo_motor_plugin.so" name="rudplugin">
      <NameOfJoint> rud </NameOfJoint>
      <Modo> Posicao </Modo>
      <TopicSubscriber> Ruddef </TopicSubscriber>
      <TopicPublisher> SensorRud </TopicPublisher>
    </plugin>

    <plugin filename="libgazebo_ros_servo_motor_plugin.so" name="elevplugin">
      <NameOfJoint> elev </NameOfJoint>
      <Modo> Posicao </Modo>
      <TopicSubscriber> Elevdef </TopicSubscriber>
      <TopicPublisher> SensorElev </TopicPublisher>
    </plugin>

    <plugin filename="libgazebo_ros_AllData_plugin.so" name="VetorEstados">   
       <bodyName> main_body </bodyName> 
       <NameOfNode> Estados </NameOfNode> 
       <NameOfJointR> aR </NameOfJointR> 
       <NameOfJointL> aL </NameOfJointL> 
     </plugin>  
    
     <plugin filename="libgazebo_ros_aerodinamica_plugin.so" name="empuxo">            
       <bodyName> main_body </bodyName> 
       <NameOfNode> Estados </NameOfNode> 
      
       <topic_Fr> ThrustR </topic_Fr> 
       <LinkFr> propeller_R </LinkFr> 
       <JointFr>  thrustdir  </JointFr> 

       <topic_Fl> ThrustL </topic_Fl> 
       <LinkFl> propeller_L </LinkFl> 
       <JointFl>  thrustesq  </JointFl> 

       <topic_Rud> posRud </topic_Rud> 
       <LinkRud> rudder </LinkRud> 
      
       <topic_Elev> posElev </topic_Elev> 
       <LinkElev> elevator </LinkElev> 
     </plugin> 

   </model> 
 </sdf> 



























































