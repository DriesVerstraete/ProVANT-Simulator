    <joint name="MainbodyRotorL" type="revolute">
      <parent>Main_body</parent>
      <child>Rotor_L</child>
      <pose>0 0 0 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <upper>1.57</upper>
          <lower>-1.57</lower>
