<?xml version="1.0" encoding="utf-8"?>
<sdf version="1.4">
  <model name="vant_quatro_lqr">
    <pose>0 0 0.0325 0 0 0</pose>
    <plugin name="clock" filename="libgz_clock.so">
      <topic>/gazebo/frc/time</topic>
    </plugin> 
    <link name="Main_body">
      <gravity>1</gravity>
      <self_collide>0</self_collide>
      <pose>0.00868363919454268 -0.00241505251115258 0.0193917686877877 0 0 0</pose> <!--Provavel pose do centro de massa com rel a referencia adotada nesse caso a origem do desenho (ucs_body) -->
      <must_be_base_link>1</must_be_base_link>
      <velocity_decay>
        <linear>0</linear>
        <angular>0</angular>
      </velocity_decay>
      <inertial>
        <mass>7</mass>
        <pose>0 0 0 0 0 0</pose>
        <inertia>
          <ixx>0.1489</ixx>
          <ixy>0.0001</ixy>
          <ixz>-0.0189</ixz>
          <iyy>0.1789</iyy>
          <iyz>-0.000029</iyz>
          <izz>0.3011</izz>
        </inertia>
      </inertial>
     <collision name="Main_body_collision">
        <pose>-0.00868363919454268 0.00241505251115258 -0.0193917686877877 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://vant_quatro_lqr/meshes/Main_body_col.STL</uri>
          </mesh>
        </geometry>
        <surface>
          <contact>
            <ode />
          </contact>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
            </ode>
          </friction>
        </surface>
      </collision> 
      <visual name="Main_body_visual">
        <pose>-0.00868363919454268 0.00241505251115258 -0.0193917686877877 0 0 0</pose>
        <material>
          <ambient>1 0 0 1</ambient>
          <diffuse>1 0 0 1</diffuse>
        </material>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://vant_quatro_lqr/meshes/Main_body.STL</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    <link name="Rotor_R">
      <gravity>1</gravity>
      <self_collide>0</self_collide>
      <pose>0.0780 -0.6073 0.1235 0 0 0 </pose>  <!-- UCS_RotorR em relação a referencia adotada nesse caso a origem do desenho (UCS_body)-->
      <must_be_base_link>0</must_be_base_link>
      <velocity_decay>
        <linear>0</linear>
        <angular>0</angular>
      </velocity_decay>
      <inertial>
        <mass>0.3</mass>
        <pose>0 0 0.03 0 0 0</pose>
        <inertia>
          <ixx>0.0007</ixx>
          <ixy>3.4E-07</ixy>
          <ixz>1.1E-05</ixz>
          <iyy>0.0007</iyy>
          <iyz>1.5E-05</iyz>
          <izz>0.0002</izz>
        </inertia>
      </inertial>
    <collision name="Rotor_R_collision">
        <pose>-0.0780 0.6073 -0.1235 0 0 0</pose> <!--pose multiplicado por -1-->
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://vant_quatro_lqr/meshes/Rotor_R_col.STL</uri>
          </mesh>
        </geometry>
        <surface>
          <contact>
            <ode />
          </contact>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
            </ode>
          </friction>
        </surface>
      </collision>  
      <visual name="Rotor_R_visual">
        <pose>-0.0780 0.6073 -0.1235 0 0 0</pose>  <!--pose do link multiplicado por -1 -->
        <material>
          <ambient>1 0 0 1</ambient>
          <diffuse>1 0 0 1</diffuse>
        </material>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://vant_quatro_lqr/meshes/Rotor_R.STL</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    <joint name="MainbodyRotorR" type="revolute">  
      <parent>Main_body</parent>     
      <child>Rotor_R</child>
      <pose>0 0 0 0 0 0 </pose>  									<!-- pose relativa da junta com relação ao frame do link (UCS_body) -->
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <upper>1.57</upper>
          <lower>-1.57</lower>
          <effort>2</effort>
        </limit>
         <dynamics>
          <damping>0</damping>
          <friction>0</friction>
        </dynamics>
      </axis>
    </joint>
    <link name="Rotor_L">
