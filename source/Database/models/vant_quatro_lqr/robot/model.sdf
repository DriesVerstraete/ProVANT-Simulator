            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="Rotor_L_visual">
        <pose>-0.0780 -0.6073 -0.1235 0 0 0</pose>
        <material>
          <ambient>1 0 0 1</ambient>
          <diffuse>1 0 0 1</diffuse>
        </material>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://vant_quatro_lqr/meshes/Rotor_L.STL</uri>
          </mesh>
