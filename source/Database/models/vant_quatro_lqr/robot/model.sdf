  <plugin name="empuxo"
filename="libgazebo_ros_brushless_plugin.so">
            <topic_FR>Thrustdirteste</topic_FR>
            <LinkDir>Rotor_R</LinkDir>
            <topic_FL>Thrustesqteste</topic_FL>
            <LinkEsq>Rotor_L</LinkEsq>
            <DragCte>0.0001</DragCte>
     </plugin>
   <plugin name="VetorEstados"
filename="libgazebo_ros_AllData_plugin.so">
            <NameOfTopic>Estados</NameOfTopic>
            <NameOfJointR>MainbodyRotorR</NameOfJointR>
            <NameOfJointL>MainbodyRotorL</NameOfJointL>
            <bodyName>Main_body</bodyName>
     </plugin>    
     <plugin name="RotorRplugin"
filename="libgazebo_ros_servo_motor_plugin.so">
            <NameOfJoint>MainbodyRotorR</NameOfJoint>   
            <TopicPublisher>RotorRteste</TopicPublisher>
            <TopicSubscriber>RefRotorRteste</TopicSubscriber>
            <Modo>Torque</Modo>
            <Saturation>1</Saturation>
     </plugin>
      <plugin name="RotorLplugin"
filename="libgazebo_ros_servo_motor_plugin.so">
            <NameOfJoint>MainbodyRotorL</NameOfJoint>
            <TopicPublisher>RotorLteste</TopicPublisher>
            <TopicSubscriber>RefRotorLteste</TopicSubscriber>
            <Modo>Torque</Modo>
            <Saturation>1</Saturation>
     </plugin>
  </model>
</sdf>
